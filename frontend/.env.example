# BioNexus Frontend Environment Variables
# Copy this file to .env.local and customize for your environment

# API Configuration
API_BASE_URL=http://localhost:8000
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000

# Application Configuration
NEXT_PUBLIC_APP_NAME=BioNexus
NEXT_PUBLIC_APP_VERSION=1.0.0

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG=false

# External Services (Optional)
NEXT_PUBLIC_WEAVIATE_URL=http://localhost:8080
NEXT_PUBLIC_NEO4J_URI=bolt://localhost:7687

# Development Settings
NODE_ENV=development